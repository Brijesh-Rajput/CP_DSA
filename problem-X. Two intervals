#include<bits/stdc++.h>
#define ll long long int
#define l long int
using namespace std;
void solve(){

}

int main(){
#ifndef ONLINE_JUDGE
	freopen("Input.txt","r",stdin); 
	freopen("Output.txt","w",stdout);
#endif

	int l1,r1,l2,r2;
	cin>>l1>>r1>>l2>>r2;

	//Total 5 cases
	if(l2>=l1 && l2<=r1 && r2>=l1 && r2<=r1) cout<<l2<<" "<<r2;
	else if(l2>=l1 && l2<=r1 && !(r2>=l1 && r2<=r1)) cout<<l2<<" "<<r1;
	else if(l1>=l2 && l1<=r2 && r1>=l2 && r1<=r2) cout<<l1<<" "<<r1;
	else if(l1>=l2 && l1<=r2 && !(r1>=l2 && r1<=r2)) cout<<l1<<" "<<r2;
	else cout<<"-1";

}
